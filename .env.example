# Trading System Configuration
# Copy this file to .env and add your actual API keys

# Required API Keys
FINNHUB_API_KEY=your_finnhub_api_key_here
ALPHA_VANTAGE_KEY=your_alpha_vantage_api_key_here

# Optional API Keys (can use "not_set" if not available)
POLYGON_API_KEY=not_set
QUIVER_API_KEY=not_set
ALPACA_API_KEY=not_set

# Database Configuration
DATABASE_URL=postgresql://localhost/trading_db
REDIS_URL=redis://localhost

# Server Configuration
SSE_PORT=8090
NEXT_PUBLIC_SSE_URL=http://localhost:8090/events

# Congressional Data Configuration  
CONGRESS_MAX_PAGES=50

# Development Flags
# COLLECT_GOVERNMENT_DATA=1  # Uncomment for Phase 1
# COLLECT_HISTORICAL=1       # Uncomment for Phase 2
# FORCE_REFRESH=1           # Uncomment to force refresh existing data

# Additional symbols to track (comma-separated)
# SYMBOLS=AAPL,MSFT,GOOGL,TSLA,NVDA,BINANCE:BTCUSDT
